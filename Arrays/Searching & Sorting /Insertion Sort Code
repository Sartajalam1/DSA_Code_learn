// insertion Sort in Array
#include <iostream>
#include <vector>
using namespace std;

int main() {
  vector<int> arr = {10,1,7,6,14,9};
  int n = arr.size();

  for (int round = 1; round < n; round++) {
    // Step A:  Fetch value
    int val = arr[round];

    // Step B: campare
    int j = round-1;
    
    for (;  j >= 0; j--) {
      if (arr[j] > val) {
         // Step C: Shift
        arr[j+1] = arr[j];
      }
      else{
        // Rukana akb hai
        break;
      }
    }
    // Step D: Copy
    arr[j+1] =val;
      
  }
// Printing
  for (int i = 0; i < n; i++) {
    cout << arr[i] << " ";
  }
  cout << endl;
}
