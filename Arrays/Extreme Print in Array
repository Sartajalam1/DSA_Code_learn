//Extreme Print in Array
#include<iostream>
using namespace std;

int main(){
  int arr[] = {1,2,3,4,5,6,7,8,9};
  //o/p {1,9,2,8,3,7,4,6,5}
  int size =9;
  int start = 0;
  int end = size-1;
//when start lessthan End continue loop;
  while(start<=end){
    
    if(start == end){
      cout<<arr[start]<<endl;
    }
    else{
      cout<<arr[start]<<" ";
      cout<<arr[end]<<" ";
    }
    start++;
    end--;
  }
  
}
