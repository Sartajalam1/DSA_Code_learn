//Pair Sum In Array
//isme check karna hai ki kiska pair milke sum banta hai
#include<iostream>
#include<vector>
using namespace std;

int main(){
  vector<int>arr{1,3,5,7,5,6};
  int sum = 9;

  for(int i =0; i<arr.size(); i++){
    for(int j = 0; j<arr.size(); j++){
      if(arr[i]+arr[j] == sum){
        cout<<"("<<arr[i]<<","<<arr[j]<<")"<<endl;
      }
    }
  }
}
