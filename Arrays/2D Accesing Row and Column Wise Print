#include<iostream>
#include<vector>
using namespace std;

int main(){
int arr[3][3] = {{1,2,3},{4,5,6},{7,8,9}};
  //outer loop 
  for(int i = 0; i<3; i++){
    // inner loop
    for(int j = 0; j<3; j++){
      //print the row wise , accessing Row-Wise
      cout<<arr[i][j]<<" ";
    }
    cout<<endl;
  }

  // Column Wise Accesing Print
  int arr[3][3] = {{1,2,3},{4,5,6},{7,8,9}};
  //outer loop 
  for(int i = 0; i<3; i++){
    // inner loop
    for(int j = 0; j<3; j++){
      //print the col wise , accessing col-Wise
      cout<<arr[j][i]<<" ";
    }
    cout<<endl;
  }
}
