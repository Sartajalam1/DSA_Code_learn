// Given Number is odd And Even
#include<iostream>
using namespace std;

bool oddEven(int num){
  if(/*num%2==0*/ (num&1)==0){
    return true;
  }
  else{
    return false;
  }
}

int main(){
  int n;
  cin>>n;
  bool find= oddEven(n);
  // cout<<"find the odd even "<<find<<endl;
  if(find){
    cout<<n<< " is Even Number./n";
  }
  else{
    cout<<n<< " is Odd Number./n";
  }
}
